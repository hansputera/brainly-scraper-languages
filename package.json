    "prepare": "husky install",
    "docs": "typedoc --footerTypedocVersion --footerDate --footerTime --favicon https://i.ibb.co/pyqPrLk/image.png",
    "format": "prettier \"src/**/*.ts\" --write"
  },
  "author": {
    "name": "Hanif Dwy Putra S",
    "email": "hanifdwyputrasembiring@gmail.com",
    "url": "https://thehanifs.tech"
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^0.27.2",
    "piscina": "^3.2.0"
  },
  "devDependencies": {
    "@types/node": "16.11.42",
    "@typescript-eslint/eslint-plugin": "5.30.6",
    "@typescript-eslint/parser": "5.30.6",
    "eslint": "8.19.0",
    "eslint-config-google": "0.14.0",
    "eslint-config-prettier": "8.5.0",
    "husky": "8.0.1",
    "prettier": "2.8.8",
    "rimraf": "3.0.2",
    "typedoc": "latest",
    "typedoc-plugin-extras": "2.2.3",
    "typescript": "4.7.4"
  },
  "repository": {
    "url": "https://github.com/hansputera/brainly-scraper-languages"
  },
