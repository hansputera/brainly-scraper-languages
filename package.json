    "docs": "typedoc --footerTypedocVersion --footerDate --footerTime --favicon https://i.ibb.co/pyqPrLk/image.png",
    "format": "prettier \"src/**/*.ts\" --write"
  },
  "author": {
    "name": "Hanif Dwy Putra S",
    "email": "hanifdwyputrasembiring@gmail.com",
    "url": "https://hdps.my.id"
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^1.1.2",
    "piscina": "^3.2.0"
  },
  "devDependencies": {
    "@types/node": "^18.8.5",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "eslint": "^8.25.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "typedoc": "latest",
    "typedoc-plugin-extras": "^2.3.0",
    "typescript": "^4.8.4"
  },
  "repository": {
